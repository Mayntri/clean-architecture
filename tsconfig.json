{
   "compilerOptions": {
     "target": "es2016",
     "module": "commonjs",
     "baseUrl": ".",
     "paths": {
       "@infrastructure/*": ["./src/infrastructure/*"]
     },
     "sourceMap": true,
     "outDir": "dist",
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
     "strict": true,
     "skipLibCheck": true
   }
 }
 